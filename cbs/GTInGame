<script type="text/javascript">
	var cv_aTmLogo = "http://sports.cbsimg.net/images/nba/logos/90x90/SA.png", cv_hTmLogo = "http://sports.cbsimg.net/images/nba/logos/90x90/WAS.png", courtImgUrl = "/s/store/-1/webapp/cbs/basketball_gt/NBA_court_view.png";
	var courtWidth = 300, courtHt = 170;
	var courtImgCtx, lastShotAtCenter = "false";
	var lastShotSelIcon = "/s/store/-1/webapp/cbs/basketball_gt/last_shot_selected.png", lastShotUnSelIcon = "/s/store/-1/webapp/cbs/basketball_gt/last_shot_unselected.png", lastShotClose = "/s/store/-1/webapp/cbs/basketball_gt/lastShotPanel_close.png";
	var lspImgURL = "/s/store/-1/webapp/cbs/basketball_gt/last_shot_panel.png";
	var size = [35, 35], size1 = [35, 35], lShotIconCoord, closeButtonCoord;
	var fNFont = "400 14px 'EXO',Arial", lNFont = "700 14px 'EXO',Arial", lsDataNFont = "Normal 11px Arial"; 
	var prevLastShot, closeEvent = "false";
	
	function initializeClickEventCoords(){
		lShotIconCoord = new Array();
		closeButtonCoord = new Array();
		var s = lastShot.side;
		var team = lastShot.team;
		var lx = lastShot.lX;
		var ly = lastShot.lY;
		lx = getXCorInPixel(lx, ly, s, team);
		ly = getYCorInPixel(ly, s, team);
		
		var coords = getCorForInfoPanel(parseInt(lx), parseInt(ly), s, team);
		
		var x1, y1;
		
		var coordsArray = coords.split("-");
		if(coordsArray.length > 0){
			x1 = coordsArray[0];
			x1 = parseInt(x1);
			y1 = coordsArray[1];
			y1 = parseInt(y1);
		}
		
		lShotIconCoord[0] = parseInt(lx) - 5;
		lShotIconCoord[1] = parseInt(ly) + 40;
		if(lastShotAtCenter == "true"){
			closeButtonCoord[0] = x1 + 90;
			closeButtonCoord[1] = 55;
		}else{
			closeButtonCoord[0] = x1 + 130;
			closeButtonCoord[1] = y1 + 40;
		}
	}
	
	initializeClickEventCoords();
	
	function createCVCanvas() {
		cbsGTCourtCanvas = document.getElementById('cbsGTCourtCanvas');	
		if (cbsGTCourtCanvas.getContext){					 
			courtImgCtx = cbsGTCourtCanvas.getContext('2d');
			var courtImg = new Image();
			courtImg.onload = function(){
				cbsGTCourtCanvas.width = courtWidth;
				cbsGTCourtCanvas.height = courtHt;
				courtImgCtx.drawImage(courtImg, 0, 0, courtWidth, courtHt);
				drawTmLogo(cv_aTmLogo, 23);
				drawTmLogo(cv_hTmLogo, parseInt(courtWidth) - 47);
				drawShots();
				drawLastShotIcon();
				if(closeEvent == "false"){
					drawLastShotInfoPanel();
				}
			}
			courtImg.src = courtImgUrl;
		}
	}
	
	function drawTmLogo(teamLogoUrl, xCor){															
		var teamLogoImg = new Image();
		teamLogoImg.onload = function(){
			courtImgCtx.globalAlpha = 1;
			courtImgCtx.drawImage(teamLogoImg, xCor, parseInt(courtHt)/2 - 20, 25, 30);				
		}
		teamLogoImg.src = teamLogoUrl;
	}
	function drawShots(){
		 //loop thru the array
		 for(var i=0; i<shotsDataArray.length; i++){
		 	drawHitMiss(shotsDataArray[i]);
		 }
	}
	function getXCorInPixel(x, y, side, team){
		var xPos = 0;
		if(team == "away"){
			xPos = Math.round(x*3.2);
			if(side == "Right"){
				if(xPos > 75)
					xPos = xPos - 13;
				else
					xPos = xPos + 8;
			}else{
				if(xPos > 75)
					xPos = xPos - 13;
				else 
					xPos = xPos;
			}
		}else{
			xPos = Math.round((courtWidth) - (x*3.2));
			if(side == "Left"){
				if(xPos > 75)
					xPos = xPos - 18;
				else
					xPos = xPos - 40;
			}else{
				if(xPos > 75)
					xPos = xPos - 18;
				else
					xPos = xPos - 35;
			}
		}
		if(xPos > 280){
			xPos = 275;
		}else if(xPos <= 13){
			xPos = 13;
		}
		return xPos;
	}
	function getYCorInPixel(y, side, team){
		var yPos;
		if(team == "home"){
			if(side == "Left"){
				yPos = Math.round((courtHt/2 - 10) - (y*3.4));
			}else{
				yPos = Math.round((courtHt/2) + (y*3.4));
			}
		}else{
			if(side == "Right"){
				yPos = Math.round((courtHt/2 - 10) - (y*3.4));
			}else{
				yPos = Math.round((courtHt/2) + (y*3.4));
			}
		}
		if(yPos <= -5){
			yPos = 0;
		}else if(yPos >= 140){
			yPos = 140;
		}
		return yPos;
	}
	function drawHitMiss(shotData){
		var pts = shotData.pts;
		var xCor = shotData.x;
		var yCor = shotData.y;
		var side = shotData.side;
		var team = shotData.team;
		xCor = getXCorInPixel(xCor, yCor, side, team, pts);
		yCor = getYCorInPixel(yCor, side, team, pts);
		
		var shotIcon = shotData.shotIcon;
		var shotIconImg = new Image();
		shotIconImg.onload = function(){
			courtImgCtx.globalAlpha = 1;
			courtImgCtx.drawImage(shotIconImg, xCor, yCor, 18, 18);				
		}
		shotIconImg.src = shotIcon;
	}
	function drawLastShotIcon(){
		var s = lastShot.side;
		var team = lastShot.team;
		var h_m = lastShot.l_h_m;
		var lx = lastShot.lX;
		var ly = lastShot.lY;
		
		lx = getXCorInPixel(lx, ly, s, team);
		ly = getYCorInPixel(ly, s, team);
		var lastShotIcon = lastShotUnSelIcon;
		if(h_m == "h"){
			lastShotIcon = lastShotSelIcon;
		}	
		var lastShotImg = new Image();
		lastShotImg.onload = function(){
			courtImgCtx.globalAlpha = 1;
			courtImgCtx.drawImage(lastShotImg, lx, ly, 30, 30);				
		}
		lastShotImg.src = lastShotIcon;
	}
	
	$("canvas").click(function(e) { 
		e.preventDefault();
		var s = lastShot.side;
		var team = lastShot.team;
		var h_m = lastShot.l_h_m;
		var lx = lastShot.lX;
		var ly = lastShot.lY;
		
		lx = getXCorInPixel(lx, ly, s, team);
		ly = getYCorInPixel(ly, s, team);
		
	    if ( testIfOver(this, e, size, lShotIconCoord) ) {
	        drawLastShotInfoPanel();                
	    }
	    if ( testIfOver(this, e, size, closeButtonCoord)) {
	    	closeEvent = "true";
			createCVCanvas();
	    }
	});
	$("canvas").mousemove(function(e) { 
	    if ( testIfOver(this, e, size, lShotIconCoord)) {
	        $(this).css("cursor", "pointer");  
	    } else if(testIfOver(this, e, size, closeButtonCoord)){
	          $(this).css("cursor", "pointer"); 
	    }else{	
	    	$(this).css("cursor", "default");
	    }
	});
	function testIfOver(ele,ev,size,coord){
	    if (
	        ev.pageX > coord[0] + ele.offsetLeft &&  
	        ev.pageX < coord[0] + size[0] + ele.offsetLeft &&
	        ev.pageY > coord[1] + ele.offsetTop &&           
	        ev.pageY < coord[1] + size[1] + ele.offsetTop
	    ) {
	        return true;
	    }
	    return false;
	}
		
	function getCorForInfoPanel(lx, ly, s, team){
		var x1 = 60, y1 = 127, x2 = 260, y2 = 227;
		if(ly > 45 && ly <= 120 && lx >= 95 && lx <= 131){
			y1 = ly - 40;
			x1 = lx + 30;
			lastShotAtCenter = "true";
		}else if(ly > 45 && ly <= 120 && lx >= 132 && lx <= 165){
			y1 = ly - 40;
			x1 = lx - 120;
			lastShotAtCenter = "true";
		}else if(ly >= -10 && ly <= 45 && lx >= 0 && lx <= 131){
			y1 = ly + 25;
			x1 = lx - (lx / 2);
			lastShotAtCenter = "false";
		}else if(ly > 45 && ly < 100 && lx >= 0 && lx < 95){
			y1 = ly - 30;
			x1 = lx + 25;
			lastShotAtCenter = "false";
		}else if(ly > 100 && ly <= 170 && lx >= 0 && lx <= 131){
			y1 = ly - 100;
			x1 = lx - (lx / 2);
			lastShotAtCenter = "false";
		}else if(ly >= -10 && ly <= 45 && lx >= 132 && lx <= 300){
			y1 = ly + 25;
			x1 = lx - (173 - (courtWidth - lx - 10));
			lastShotAtCenter = "false";
		}else if(ly > 45 && ly < 100 && lx > 165 && lx <= 300){
			y1 = ly - 35;
			x1 = lx - 170;
			lastShotAtCenter = "false";
		}else if(ly > 100 && ly <= 170 && lx >= 132 && lx <= 300){
			y1 = ly - 100;
			x1 = lx - (173 - (courtWidth - lx - 10));
			lastShotAtCenter = "false";
		}
		return x1 + "-" + y1;
	}
	
	function drawLastShotInfoPanel(){
		var pImgURL = lastShot.pImg;
		var fN = lastShot.pFN;
		var lN = lastShot.pLN;
		lN = $.trim(lN);
		var lN1 = "", lN2 = "";
		if(lN.indexOf(" ") == -1){
			lN1 = lN;
		}else{
			var lNStrings = lN.split(" ");
			if(lNStrings.length > 1){
				lN1 = lNStrings[0];
				lN2 = lNStrings[1];
				if(lN2.length > 9){
					lN2 = lN2.substring(0,8);
					lN2 = lN2 + "."
				}
			}else{
				lN1 = lN;
			}
		}
		var qtr = lastShot.q;
		var gs = lastShot.gstats;
		var lst = lastShot.sType;
		
		var s = lastShot.side;
		var team = lastShot.team;
		var lx = lastShot.lX;
		var ly = lastShot.lY;
		lx = getXCorInPixel(lx, ly, s, team);
		ly = getYCorInPixel(ly, s, team);
		
		var coords = getCorForInfoPanel(parseInt(lx), parseInt(ly), s, team);
		
		var x1, y1;
		var coordsArray = coords.split("-");
		if(coordsArray.length > 0){
			x1 = coordsArray[0];
			x1 = parseInt(x1);
			y1 = coordsArray[1];
			y1 = parseInt(y1);
		}
		
		var lastShotPanelImg = new Image();
		lastShotPanelImg.onload = function(){
			if(lastShotAtCenter == "true"){
				var yCoord = 15;
				courtImgCtx.drawImage(lastShotPanelImg, x1, yCoord, 120, 135);
				var plyrImg = new Image();
				plyrImg.onload = function(){
					courtImgCtx.drawImage(plyrImg, x1 + 5, yCoord + 5, 45, 55);
				}
				plyrImg.src = pImgURL;
				
				var plyrImgErr = new Image();
				plyrImgErr.src = "http://sports.cbsimg.net/images/players/unknown_player.gif";
				plyrImg.onerror = function(){
					courtImgCtx.drawImage(plyrImgErr, x1 + 5, yCoord + 5, 45, 55);
				}				
				courtImgCtx.font = fNFont;
				courtImgCtx.textAlign = "left";
				courtImgCtx.fillStyle = "#444444";
				courtImgCtx.fillText(fN, x1 + 55, yCoord + 30);
				courtImgCtx.font = lNFont;
				courtImgCtx.fillText("X", x1 + 100, yCoord + 20);
				if(lN2 != "" && lN2 != undefined){
					courtImgCtx.fillText(lN1, x1 + 55, yCoord + 45);
					courtImgCtx.fillText(lN2, x1 + 55, yCoord + 60);
				}else{
					courtImgCtx.fillText(lN1, x1 + 55, yCoord + 45);
				}
				courtImgCtx.font = lsDataNFont;
				courtImgCtx.fillText(qtr, x1 + 5, yCoord + 75);
				courtImgCtx.font = lsDataNFont;
				courtImgCtx.fillText(lst, x1 + 5, yCoord + 90);
				
				courtImgCtx.beginPath();
				courtImgCtx.moveTo(x1 + 5, yCoord + 95);
				courtImgCtx.lineTo(x1 + 115, yCoord + 95);
				courtImgCtx.lineWidth = 1;
				// set line color
				courtImgCtx.strokeStyle = '#bbbbbb';
				courtImgCtx.stroke();
				courtImgCtx.closePath();
				
				courtImgCtx.fillStyle = "#444444";
				courtImgCtx.font = lsDataNFont;
				var gs1 = "", gs2 = "";
				if(gs.indexOf("|") != -1){
					var gsArray = gs.split("|")
					if(gsArray.length > 0){
						if(gsArray.length == 3){
							gs1 = gsArray[0] +" | "+ gsArray[1] + " |";
							gs2 = gsArray[2];
							gs2 = $.trim(gs2);
						}else if(gsArray.length == 2){
							gs1 = gsArray[0] +" | "+gsArray[1];
						}else{
							gs1 = gs;
						}
					}
				}
				courtImgCtx.fillText(gs1, x1 + 5, yCoord + 110);
				if(gs2 != "" && gs2 != undefined){
					courtImgCtx.fillText(gs2, x1 + 5, yCoord + 125);
				}
			}else{
				courtImgCtx.drawImage(lastShotPanelImg, x1, y1, 173, 100);
				var plyrImg = new Image();
				plyrImg.onload = function(){
					courtImgCtx.drawImage(plyrImg, x1 + 5, y1 + 5, 45, 55);
				}
				plyrImg.src = pImgURL;
				
				var plyrImgErr = new Image();
				plyrImgErr.src = "http://sports.cbsimg.net/images/players/unknown_player.gif";
				plyrImg.onerror = function(){
					courtImgCtx.drawImage(plyrImgErr, x1 + 5, y1 + 5, 45, 55);
				}
				
				courtImgCtx.font = fNFont;
				courtImgCtx.textAlign = "left";
				courtImgCtx.fillStyle = "#444444";
				
				if(lN2 != "" && lN2 != undefined){
					courtImgCtx.fillText(fN, x1 + 55, y1 + 16);
					courtImgCtx.font = lNFont;
					courtImgCtx.fillText("X", x1 + 150, y1 + 20);
					courtImgCtx.fillText(lN1, x1 + 55, y1 + 31);
					courtImgCtx.fillText(lN2, x1 + 55, y1 + 46);
					courtImgCtx.font = lsDataNFont;
					courtImgCtx.fillText(qtr, x1 + 55, y1 + 58);
				}else{
					courtImgCtx.fillText(fN, x1 + 55, y1 + 25);
					courtImgCtx.font = lNFont;
					courtImgCtx.fillText("X", x1 + 150, y1 + 20);
					courtImgCtx.fillText(lN1, x1 + 55, y1 + 40);
					courtImgCtx.font = lsDataNFont;
					courtImgCtx.fillText(qtr, x1 + 55, y1 + 55);
				}
				courtImgCtx.font = lsDataNFont;
				courtImgCtx.fillText(lst, x1 + 7, y1 + 73);
				
				courtImgCtx.beginPath();
				courtImgCtx.moveTo(x1 + 5, y1 + 77);
				courtImgCtx.lineTo(x1 + 165, y1 + 77);
				courtImgCtx.lineWidth = 1;
				// set line color
				courtImgCtx.strokeStyle = '#bbbbbb';
				courtImgCtx.stroke();
				courtImgCtx.closePath();
				
				courtImgCtx.fillStyle = "#444444";
				courtImgCtx.font = lsDataNFont;
				courtImgCtx.fillText(gs, x1 + 7, y1 + 89);
			}
		}
		lastShotPanelImg.src = lspImgURL;
	}
	
	createCVCanvas();
	
	$(document).ready(function() {
		if("FINAL" == "INPROGRESS"){
			autoRefreshNBAGTInGame('/s/data.do?pageId=GTInGameAJAX&siteId=107433&gId=1997092&gs=FINAL&gDate=20121126&selStats=CV&platform=ajax', processResponse, 'cbsNBAGTInGame', 30000);
		}
	});
	
	var nbaGTInGmAR = null; 
	
	getChartForQuarter = function(url, selQuart, curQuart){
		var gtGameStatus = $("#gameStatusId").val();
		var gtURL = url;
		if(selQuart != curQuart){
			if(nbaGTInGmAR != null && nbaGTInGmAR != undefined){
				nbaGTInGmAR.stop();
			}
			if(gtURL != null && gtURL != undefined){ 
				loadContentThroughAjax(gtURL, processResponse, 'cbsNBAGTInGame', 30000);
			}
		}else{
			if(gtURL != null && gtURL != undefined){ 
				loadContentThroughAjax(gtURL, processResponse, 'cbsNBAGTInGame', 30000);
			}
			if(nbaGTInGmAR != null && nbaGTInGmAR != undefined){
				nbaGTInGmAR.stop();
			}
			if(gtGameStatus != null && gtGameStatus != undefined && gtGameStatus == "INPROGRESS"){
				autoRefreshNBAGTInGame(gtURL, processResponse, 'cbsNBAGTInGame', 30000);
			}
		}
	}

	autoRefreshNBAGTInGame = function(url, callback, containerId, interval){
		nbaGTInGmAR = new AutoRefresh(url, containerId, callback, function(){console.log('ERROR : Failed to load autoRefreshAllScoreData')}, interval);
		nbaGTInGmAR.init();
	}
	updateBasketballLSData = function(html){
		$("#nbaLsContainer").html(html);
	}
	updateLast5PlaysData = function(html){
		$("#last5PlaysContainer").html(html);
	}
	updateQuarterSelector = function(html){
		$("#quarterSelector").html(html);
	}
	updateCVData = function(html){
		$("#cvShotsParamContainer").html(html);
		var shotsParams = $("#cvShotsDataId").text().trim();
		var lastShotDatas = $("#lastShotDataId").text().trim();
		var shotsArray = shotsParams.split('|');
		if(shotsArray.length > 0){
			shotsDataArray = new Array();
			for(var j=0;j<shotsArray.length;j++){
				var shotDatas = shotsArray[j];
				if(shotDatas != ''){
					var shotData = new shotsData();
					var sDatas = shotDatas.split(',');
					if(sDatas.length > 0){
						shotData.pts = sDatas[0].trim();
						shotData.x = sDatas[1].trim();
						shotData.y = sDatas[2].trim();
						shotData.side = sDatas[3].trim();
						shotData.team = sDatas[4].trim();
						shotData.shotIcon = sDatas[5].trim();
						shotsDataArray[j] = shotData;
					}
				}
			}
		}
		var lastShotDataArray = lastShotDatas.split(',');
		if(lastShotDataArray.length > 0){
			lastShot = new lastShotData();
			lastShot.lX = lastShotDataArray[0].trim();
			lastShot.lY = lastShotDataArray[1].trim();
			lastShot.side = lastShotDataArray[2].trim();
			lastShot.team = lastShotDataArray[3].trim();
			lastShot.pImg = lastShotDataArray[4].trim();
			lastShot.pFN = lastShotDataArray[5].trim();
			lastShot.pLN = lastShotDataArray[6].trim();
			lastShot.q = lastShotDataArray[7].trim();
			lastShot.l_h_m = lastShotDataArray[8].trim();
			lastShot.gstats = lastShotDataArray[9].trim();
			lastShot.sType = lastShotDataArray[10].trim();
		}
		courtImgCtx.clearRect(0,0, cbsGTCourtCanvas.width, cbsGTCourtCanvas.height);
		closeEvent = "false";
		initializeClickEventCoords();
		createCVCanvas();
	}
</script>

