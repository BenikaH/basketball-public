<script type="text/javascript">
	$(document).ready(function(){  
		if(selectedArena == "" || selectedArena == "undefined"){
			$('#hdrRtLabel').removeClass("hdr_selectedMenu");
		}else{
			$('#hdrRtLabel').addClass("hdr_selectedMenu");
		}
		if(selectedArena == "FANTASY"){
			$('.hdr_selectedMenu').text("MOBILE APPS")
		}else{
			$('.hdr_selectedMenu').text(selectedArena);
		}
		menuContMinHt = parseInt($("#cbsMenuContainer").height());
	});
</script>
